{
  "name": "nextjs-netlifycms-typescript",
  "version": "0.1.1",
  "description": "Boilerplate for nextjs app using netlify CMS, tailwind, prettier, next optimized images and typescript.",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "export": "next build && next export",
    "lint": "eslint --ext .ts,.tsx",
    "prettier": "prettier --write \"(pages|components)/**/*.{js,jsx,ts,tsx}\"",
    "local-backend": "npx netlify-cms-proxy-server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "aos": "^2.3.4",
    "next": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^5.0.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "image-trace-loader": "^1.0.2",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "jimp": "^0.16.1",
    "lqip-loader": "^2.2.1",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "responsive-loader": "^2.2.0",
    "sass": "^1.28.0",
    "svg-sprite-loader": "^5.0.0",
    "tailwindcss": "^1.9.5",
    "typescript": "4.0",
    "webp-loader": "^0.6.0"
  },
  "license": "MIT",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "parser": "typescript"
  }
}
